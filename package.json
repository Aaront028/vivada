{
  "name": "vivada-ui",
  "version": "0.0.0",
  "scripts": {
    "install": "npm install force --legacy-peer-deps ng build --configuration=production",
    "ng": "ng",
    "start": "ng serve",
    "start:testing": "ng serve --configuration testing",
    "build:staging": "rm dist -rf && ng build --configuration staging && npm run scully",
    "build:testing": "rm dist -rf && ng build --configuration testing",
    "build:production": "rm dist -rf && ng build --configuration production && npm run scully",
    "watch": "ng build --watch --configuration development",
    "test": "cypress open --config defaultCommandTimeout=5000 --env apiUrl=http://localhost:8000/graphql/",
    "scully": "npx scully --",
    "scully-serve": "scully serve",
    "deploy:staging": "npm run build:staging && git add . && git commit -m'Rebuild and deploy' && git push origin staging",
    "deploy:production": "npm run build:production && git add . && git commit -m'Rebuild and deploy' && git push origin production",
    "stage": "git checkout staging && git pull origin staging",
    "golive": "git checkout staging && git pull origin staging && git checkout master && git merge staging && git push origin master && git checkout dev && git merge master && git push origin dev"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^10.2.5",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^10.2.5",
    "@angular/forms": "^10.2.5",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@ngxs/devtools-plugin": "^3.8.1",
    "@ngxs/store": "^3.8.1",
    "apollo-angular": "^5.0.2",
    "apollo-angular-link-http": "^1.11.0",
    "apollo-link-ws": "^1.0.20",
    "auto-i18n": "^1.0.0",
    "dotenv": "^16.3.1",
    "google-translate-api": "^2.3.0",
    "graphql": "^16.8.1",
    "keycloak-angular": "^14.1.0",
    "keycloak-js": "^22.0.4",
    "rxjs": "^6.5.3",
    "subscriptions-transport-ws": "^0.11.0",
    "translate": "^2.0.2",
    "translate-google": "^1.5.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.5",
    "@angular/cli": "^16.2.5",
    "@angular/compiler-cli": "^16.2.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^20.8.6",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.1.3"
  },
  "engines": {
    "npm": ">=6.0.0", 
    "node": ">=12.0.0"
  }
}
